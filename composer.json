{
	"name": "rs/form-laravel",
	"description": "Laravel Form",
	"minimum-stability": "dev",
	"require": {
		"php": "^7.3|^8.0",
		"illuminate/contracts": "^7.0|^8.0"
	},
	"autoload": {
		"psr-4": {
			"RS\\Form\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"RS\\Form\\Tests\\": "tests/"
		}
	},
	"authors": [{
		"name": "Red Snapper Development Team",
		"email": "contact@redsnapper.net"
	}],
	"require-dev": {
		"orchestra/testbench": "^5.0|^6.0",
		"laravel/legacy-factories": "^1.0@dev"
	},
	"extra": {
		"laravel": {
			"providers": [
				"RS\\Form\\FormServiceProvider"
			]
		}
	}
}
