{
	"name": "rs/form-laravel",
	"description": "Laravel Form",
	"minimum-stability": "dev",
	"require": {
		"php": "^7.1"
	},
	"autoload": {
		"psr-4": {
			"RS\\Form\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"RS\\Form\\Tests\\": "tests/"
		}
	},
	"authors": [{
		"name": "Red Snapper Development Team",
		"email": "contact@redsnapper.net"
	}],
	"require-dev": {
		"mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.4",
		"orchestra/testbench": "^4.0"
	},
	"extra": {
		"laravel": {
			"providers": [
				"RS\\Form\\FormServiceProvider"
			]
		}
	}
}
