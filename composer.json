{
	"name": "rs/form-laravel",
	"description": "Laravel Form",
	"minimum-stability": "dev",
	"require": {
		"php": "^7.1",
		"illuminate/support": "5.6.*"
	},
	"autoload": {
		"psr-4": {
			"RS\\Form\\": "src"
		}
	},
    "autoload-dev": {
        "psr-4": {
            "RS\\Form\\Tests\\": "tests/"
        }
    },
	"authors": [{
		"name": "Red Snapper Development Team",
		"email": "contact@redsnapper.net"
	}],
	"require-dev": {
		"phpunit/phpunit": "^7.0",
		"orchestra/testbench": "^3.6",
		"mockery/mockery": "^1.0"
	},
    "extra": {
        "laravel": {
            "providers": [
                "RS\\Form\\FormServiceProvider"
            ]
        }
    }
}
